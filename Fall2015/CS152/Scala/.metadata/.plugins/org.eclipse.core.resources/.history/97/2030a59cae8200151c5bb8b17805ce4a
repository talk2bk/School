

/**
 * @author CuTs
 */
class Item(private val id: Int = Item.nextId, private var description: Description) {
override def toString = "(id: " + id +", " + "Description: " +description +")" 

}

object Item{
private var  currentId = 0
def  description = ""
def apply(id: Int, description: Description) = {
new Item( description)
}
def nextId = {currentId += 1}


}