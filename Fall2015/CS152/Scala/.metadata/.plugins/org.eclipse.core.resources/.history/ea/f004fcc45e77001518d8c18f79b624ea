object midtermSession {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
 def approxZero(num: Double): Boolean = num < .1 && num > - .1
                                                  //> approxZero: (num: Double)Boolean
def countRoots(f: Double => Double, inputs: List[Double]) = inputs.map(f).filter(approxZero(_)).length
                                                  //> countRoots: (f: Double => Double, inputs: List[Double])Int










}