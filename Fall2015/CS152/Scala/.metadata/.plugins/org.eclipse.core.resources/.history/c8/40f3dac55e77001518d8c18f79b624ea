object midtermSession {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
 def approxZero(num: Any): Boolean = num.asInstanceOf[Comparable[Any]].compareTo(0.1) < 1 && num.asInstanceOf[Comparable[Any]].compareTo(-0.1) > 1
                                                  //> approxZero: (num: Double)Boolean
def countRoots[T](f: T => Double, inputs: List[T]) = inputs.map(f).filter(approxZero(_)).length
                                                  //> countRoots: (f: Double => Double, inputs: List[Double])Int










}