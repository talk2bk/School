

/**
 * @author CuTs
 */
class Item( private var description: Description) {
private var id = Item.nextId+1
override def toString = "(id: " + id +", " + "Description: " +description +")" 

}

object Item{
def id = 0
def  description = ""
def apply(id: Int, description: Description) = {
new Item( description)
}
def nextId = id = id + 1

}